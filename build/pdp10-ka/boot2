set console wru=034
set cpu its
set cpu 1024k
set cpu mpx
set cpu ten11
set rpa dis
set rpb dis
set tua dis
set fha dis
set dpb dis
set lpt dis
set cr dis
set dc dis
set dtc dis
set dk dis
at ptr bin/ka10/boot/dskdmp.rim
at mta0 out/pdp10-ka/sources.tape
set mta mpx=7
set mta type=b
at dpa0 out/pdp10-ka/rp03.2
at dpa1 out/pdp10-ka/rp03.3
at dpa2 out/pdp10-ka/rp03.0
at dpa3 out/pdp10-ka/rp03.1
set dpa noheaders
b ptr
