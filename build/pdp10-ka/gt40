set cpu 11/05
set rha disabled
set lpt disabled
set dz disabled
set rk disabled
set rl disabled
set hk disabled
set rx disabled
set rp disabled
set rq disabled
set tm disabled
set tq disabled
set cpu 16k
set tti 8b
set dli enabled
set dlo enabled
set dli lines=1
set dli address=17775610
set dlo 8b
at dli 10005,connect=localhost:10019
set vt enable
set vt crt=vr14
set vt scale=1
set vt address=17772000
set vt vec=320
set rom enabled
set rom0 address=17766000
at rom0 out/pdp10-ka/bootvt.img
boot rom0
