set console wru=034
set cpu its
set cpu 2048k
set rpb dis
set tua dis
set lpt dis
set dc disable
set pd ena
set pd on
set mta enabled type=b
at mta0 out/pdp10-kl/minsrc.tape
set rpa0 rp04
set rpa1 rp04
set rpa2 rp04
at rpa0 out/pdp10-kl/rp04.0
at rpa1 out/pdp10-kl/rp04.1
at rpa2 out/pdp10-kl/rp04.2
load -i bin/kl10/boot/@.ddt
go
