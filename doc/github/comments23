    "url": "https://api.github.com/repos/PDP-10/its/issues/comments/375990231",
    "html_url": "https://github.com/PDP-10/its/issues/410#issuecomment-375990231",
    "issue_url": "https://api.github.com/repos/PDP-10/its/issues/410",
