; -*-MIDAS-*-

TITLE LISP ITS Filesystem definitions

.INSRT SYS:.FASL DEFS

.FASL

VERPRT LFSDEF

define DEFSYM /ARG

  ARG

  IRPW FUG,,[ARG]

  IRPS x,y,[ FUG ]
    DEFLSYM X,\<X>
    .ISTOP
  TERMIN
  TERMIN
TERMIN


DEFINE DEFLSYM X,Y

IF2 [PRINTX |(DEFVAR FS-X #o!Y)
|
]
  .SXEVAL (SETQ FS-X #<Y> )
  .SXEVAL (COND ((STATUS FEATURE COMPLR)
		 (SPECIAL FS-X)))
TERMIN

.INSRT SYSENG;FSDEFS >

FASEND
