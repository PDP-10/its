	((AND (EQ $TRIGINVERSES '$ALL) (NOT (ATOM Y))
	      (IF (EQ '%COS (CAAR Y)) (CADR Y))))
	($LOGARC (LOGARC '%ACOS Y))
	((AND $TRIGSIGN (MMINUSP* Y)) (SUB '$%PI (CONS-EXP '%ACOS (NEG Y))))
