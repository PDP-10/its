	((AND $%PIARGS (IF (ZEROP1 Y) 1)))
	((AND $%IARGS (MULTIPLEP Y '$%I)) (CONS-EXP '%COS (COEFF Y '$%I 1)))
	((AND $TRIGINVERSES (NOT (ATOM Y)) (IF (EQ '%ACOSH (CAAR Y)) (CADR Y))))
	((AND $TRIGEXPAND (TRIGEXPAND '%COSH Y)))
