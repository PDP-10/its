(COMMENT DOCTOR PART ONE - THE BASIC FUNCTIONS)

(COMMENT UNUSUAL READER OPTIONS - 
		THE FOLLOWING CHARACTERS ARE TREATED SYNTACTICALLY
		JUST LIKE ALPHABETIC LETTERS: ! /" /# $ % & * = < > ? :
		NORMALLY/, SINGLE-QUOTE AND SEMI-COLON HAVE SPECIAL 
		SIGNIFICANCES/, BUT THE SETSYNTAX FUNCTION IS CALLED TO 
		OVERRIDE THEM FOR OTHER ODBALL CHARACTERS/, SLASH (IE //) 
		ACTS AS A CHARACTER QUOTING DEVICE  THE ASCII CHARACTER 
		FOLLOWING IT IS TAKEN LITERALLY/, NAMELY SYNTACTICALLY THE 
		SAME AS AN ALPHABETIC LETTER  ALSO/, INPUT BASE FOR STRINGS 
		OF DIGITS IS NORMALLY EIGHT INSTEAD OF TEN
	)


(COMMENT UNUSUAL FUNCTIONS ARE : 
	COMMENT	-	FSUBR WHICH PRODUCES THE ATOM COMMENT AS ITS VALUE
	ASCII 	-	USED TO GENERATE CHARACTER OBJECTS WITH BIZZARE PNAMES 
	DEFUN	-	USED FOR DEFINING FUNCTIONS/, FIRST ARG IS FUNCTION NAME/,
			SECOND IS LAMBDA LIST/, THIRD IS FUNCTION EXPRESSION
	PRIN1	-	PRINTS ONE S-EXPRESSION/, INSERTING SLASHES
			BEFORE ANY SYNTACTICALLY ILLEGAL CHARACTER IN A PNAME
	PRINC	-	PRINT WITHOUT USING SPECIAL ESCAPE CHARACTERS
			IN ATOMS WITH ILLEGAL CHARACTERS IN PNAME
	FLATSIZE - 	TOTAL NUMBER OF CHARS OUTPUT BY PRIN1 FOR ARGUMENT
	FLATC   - 	TOTAL NUMBER OF CHARS OUTPUT BY PRINC FOR ARGUMENT
	PRINT	-	(LAMBDA (X) 
			    (PROG NIL
				(TERPRI)
				(PRIN1 X)
				(PRINC SPACE)
				(RETURN X)))
	READLINE - 	TAKES IN AN INPUT LINE OF CHARACTERS/, RETURNING IT
			AS A SYMBOL WHOSE PNAME IS COMPOSED OF THOSE CHARS
	MAPLIST	-	HAS ARGUMENTS REVERSED FROM CONVENTION OF LISP 1/.5
			(MAPLIST FUNCTION ARGLIST)
	MAPCAR	-	(LAMBDA (FN L) 
			     (MAPLIST (FUNCTION (LAMBDA (X) (FN (CAR X)))) L))
	MAPC	-	SIMPLY MAPCAR/, BUT NO LIST IS CONSED-UP FOR RETURN
			VALUE - EXECUTED PURELY FOR ITS SIDE-EFFECT
	PUTPROP	-	(PUTPROP ATOM  VALUE INDICATOR)
	DEFPROP	-	ESSENTIALLY A FEXPR VERSION OF PUTPROP/,
			(DEFPROP A B C) = (PUTPROP (QUOTE A) (QUOTE B) (QUOTE C))
	GCTWA - 	SETS SWITCH TO PERMIT GARBAGE COLLECTION OF TRULY WORTHLESS ATOMS
	SETSYNTAX - 	MODIFIES SYNTAX PROPERTIES OF CHARACTERS FOR THE READ FUNCTION
	)

(DEFUN GOBBLE-LINES-OF-CHARS NIL
     (PROG (CH L)
	   (COND ((NOT (STATUS FEATURE NEWIO)) (GO C)))
      A0   (AND (ZEROP (FLATC (SETQ L (READLINE)))) (GO A0))
	   (SETQ L (NREVERSE (EXPLODEC L)))
      A1   (SETQ L (CONS CARRIAGERETURN L))
	   (COND ((NOT (ZEROP (FLATC (SETQ CH (READLINE)))))
		  (SETQ L (NRECONC (EXPLODEC CH) L))
		  (GO A1)))
	   (RETURN (NREVERSE (CONS CARRIAGERETURN L)))
      C    (SETQ CH (READCH))
           (COND ((AND (EQ CH CARRIAGERETURN)
			(SETQ L (CONS CH L))
			(OR (EQ (SETQ CH (READCH)) CARRIAGERETURN)
			    (AND (EQ CH LINEFEED)
				 (EQ (SETQ CH (READCH)) CARRIAGERETURN))))
		  (RETURN (NREVERSE (CONS CH L))))
		((EQ CH RUBOUT)
		  (COND (L (PRINC (CAR L))  (SETQ L (CDR L)))
			(T (TERPRI))))
		((EQ CH LINEFEED))
		((SETQ L (CONS CH L))))
	   (GO C) ))


(DEFUN WORKER NIL
 ((LAMBDA (READTABLE)
	(COMMENT NEXT FEW LINES ARE SYSTEMIC SET UP PECULIAR TO MACLISP)
	(AND (SETQ TERMINALWIDTH (STATUS TTY))
	     (NOT (ATOM TERMINALWIDTH))
	     (SSTATUS TTY (CAR TERMINALWIDTH) 
		          (BOOLE 7 (CADR TERMINALWIDTH) 8192.)))
	(AND (SETQ TERMINALWIDTH (STATUS TTYSIZE))
	     (SETQ TERMINALWIDTH (CDR TERMINALWIDTH)))
	(SETSYNTAX 39. (QUOTE MACRO) NIL)
	(SETSYNTAX 59. (QUOTE MACRO) NIL)
	(GCTWA T)
	(PROG (SENTENCE KEYSTACK)
	      (TERPRI)
	      (PRINC (QUOTE SPEAK/ UP!))
	      (TERPRI)
	  A   (SETQ SENTENCE (SETQ KEYSTACK NIL))
	      (READIN)
	      (ANALYZE)
	      (TERPRI)
	      (TERPRI)
	      (GO A)) )
   (ARRAY NIL READTABLE)))



(COMMENT FROM HERE ON IS PURE STANDARD LISP)


(DEFUN READIN NIL 
   (PROG (WORD LETTER FLAG TERMINAL LINE)
         (SETQ LINE (GOBBLE-LINES-OF-CHARS))
      A  (COND ((NULL (READWORD)) (GO B)))
         (MAKESENTENCE)
	 (SETKEYSTACK)
      B  (BREAKANALYZE)
	 (COND ((NOT FLAG) (GO A)))
	 (SETQ SENTENCE (NREVERSE SENTENCE)) ))

(DEFUN READWORD NIL 
 (PROG NIL
       (SETQ WORD NIL)
  A    (COND ((SETQ FLAG (GET (NEXTCH) (QUOTE BREAK)))
	      (RETURN (COND (WORD (SETQ WORD
					(IMPLODE (REVERSE WORD))))))))
       (SETQ WORD (CONS LETTER WORD))
       (GO A)))

(DEFUN MAKESENTENCE NIL 
 (SETQ SENTENCE
       (CONS (COND ((SETQ FLAG (GET WORD (QUOTE TRANSLATION)))
			    FLAG)
		   (WORD))
	     SENTENCE)))

(DEFUN SETKEYSTACK NIL 
 (COND ((AND (SETQ FLAG (GET WORD (QUOTE PRIORITY)))
	     KEYSTACK
	     (GREATERP FLAG
		       (GET (CAR KEYSTACK) (QUOTE PRIORITY))))
	(SETQ KEYSTACK (CONS WORD KEYSTACK)))
       (FLAG (SETQ KEYSTACK (APPEND KEYSTACK (LIST WORD))))))

(DEFUN BREAKANALYZE NIL 
 (COND ((EQ LETTER CARRIAGERETURN) (SETQ FLAG TERMINAL)
					(SETQ TERMINAL T))
       ((AND 	(SETQ FLAG (GET LETTER (QUOTE PUNCTUATION)))
		KEYSTACK)
		(GOBBLE))
       (FLAG (SETQ SENTENCE (SETQ FLAG NIL)))
       ((NOT (EQ LETTER LINEFEED))
	    (SETQ TERMINAL NIL))))

(DEFUN TEST (D S)
 (PROG NIL
  G    (COND ((NULL D)
	      (RETURN (COND ((NOT S)
			     (SETQ PARSELIST
				   (REVERSE PARSELIST))))))
	     ((NOT (COND ((NUMBERP (CAR D))
			  (COND ((ZEROP (CAR D)) (TEST5))
				((TEST3 (CAR D) NIL))))
			 ((TEST4 (CAR D)) (TEST2))))
	      (RETURN NIL)))
       (SETQ D (CDR D))
       (GO G)))

(DEFUN ADVANCE NIL 
     (RPLACA (CDAR RULES)
	     (COND ((NULL (CDADAR RULES)) (CDDAR RULES))
		   ((CDADAR RULES)))))

(DEFUN SENTPRINT (ANS)
       (PROG (N)
	  A0 (SETQ N 0)
	  A  (PRINC (CAR ANS))
	     (SETQ N (PLUS N (FLATSIZE (CAR ANS))))
	     (COND ((SETQ ANS (CDR ANS))
		    (COND ((GREATERP N TERMINALWIDTH)
			   (TERPRI) 
			   (GO A0))
			  ((PRINC (QUOTE / ))))
		    (GO A)))
	     (MEMORY)))

(DEFUN RECONSTRUCT (R)
     (COND ((NULL R) NIL)
	   ((NUMBERP (CAR R)) (APPEND (RECO1 (CAR R) PARSELIST)
				      (RECONSTRUCT (CDR R))))
	   ((CONS (CAR R) (RECONSTRUCT (CDR R))))))

(DEFUN GOBBLE NIL 
 (PROG NIL
      A	   (NEXTCH)
	   (BREAKANALYZE)
	   (COND ((NOT FLAG) (GO A)))))

(DEFUN NEXTCH NIL
	(SETQ LETTER (CAR LINE))
	(SETQ LINE (CDR LINE))
	LETTER)

(DEFUN TEST1 (PROPL X)
   (COND ((NULL PROPL) NIL)
	 ((GET X (CAR PROPL)) T)
		 ((TEST1 (CDR PROPL) X))))

(DEFUN TEST2 NIL 
  (PROG NIL
   (SETQ PARSELIST (CONS (LIST (CAR S)) PARSELIST))
   (SETQ S (CDR S))
   (RETURN T)))

(DEFUN TEST3 (X L)
	 (COND ((ZEROP X) (SETQ PARSELIST (CONS (REVERSE L) PARSELIST)))
	       (S (TEST3 (SUB1 X)
			 (CONS (CAR S) (PROG2 (SETQ S (CDR S)) L))))))

(DEFUN TEST4 (D)
     (COND ((NULL S) NIL)
	   ((ATOM D) (EQ D (CAR S)))
	   ((CAR D) (MEMBER (CAR S) D))
	   ((TEST1 (CDR D) (CAR S)))))

(DEFUN TEST5 NIL 
 (PROG (L)
       (COND ((NULL (CDR D)) (SETQ PARSELIST (CONS S PARSELIST))
			     (RETURN (NOT (SETQ S NIL)))))
  A    (COND ((TEST4 (CADR D))
	      (RETURN (SETQ PARSELIST
			    (CONS (REVERSE L) PARSELIST))))
	     ((AND (SETQ L (CONS (CAR S) L)) (SETQ S (CDR S)))
	      (GO A)))))

(DEFUN RECO1 (X P)
  (COND ((GREATERP X 1) 
	 (RECO1 (SUB1 X) (CDR P))) 
	((CAR P))))

(DEFUN ANALYZE NIL 
    (PROG (RULES PARSELIST DECOMP)
	(SETQ KEYSTACK
	      (APPEND KEYSTACK
		      (LIST (GET (QUOTE NONE)
				 (COND ((ZEROP (SETQ FLIPFLOP
						     (DIFFERENCE 2 FLIPFLOP)))
					(QUOTE MEM))
				       ((QUOTE LASTRESORT)))))))
   A	(SETQ RULES (GET (CAR KEYSTACK) (QUOTE RULES)))
   B	(SETQ DECOMP (CAAR (COND ((ATOM (CAR RULES))
				  (SETQ RULES (GET (CAR RULES) (QUOTE RULES))))
				 (RULES))))
	(SETQ PARSELIST NIL)
	(COND ((NOT (TEST DECOMP SENTENCE)) (SETQ RULES (CDR RULES)))
	      ((AND (NOT (ATOM (CAR (SETQ RULES (CAR (ADVANCE))))))
		    (NOT (EQ (CAAR RULES) (QUOTE PRE))))
	       (RETURN (SENTPRINT (RECONSTRUCT (CAR RULES)))))
	      ((NOT (ATOM (CAR RULES)))
	       (SETQ SENTENCE (RECONSTRUCT (CADAR RULES)))
	       (SETQ RULES (CDDAR RULES)))
	      ((EQ (CAR RULES) (QUOTE NEWKEY)) (SETQ KEYSTACK (CDR KEYSTACK))
					       (GO A)))
	(GO B)))


(DEFUN MEMORY NIL 
    ((LAMBDA (PARSELIST)
	     (AND (SETQ RULES (GET (CAR KEYSTACK) (QUOTE MEMR)))
		  (TEST (CAAR RULES) SENTENCE)
		  ((LAMBDA (X) (RPLACA X
				       (APPEND (CAR X)
					       (LIST (RECONSTRUCT (CAAR (ADVANCE)))))))
			(CDAR (GET (GET (QUOTE NONE) (QUOTE MEM)) (QUOTE RULES))))))
	NIL))



