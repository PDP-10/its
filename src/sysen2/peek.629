	ONUM 3,C
	CTYPE 40
	6TYPE @UNAME
	CTYPE 40
	6TYPE @JNAME
TTYLN1:	ALIGN 23.
	caile a,11
	  addi a,1
	CAIN A,11
	JRST [MOVE @TTYOPT
	      TRNN 20000
	      MOVEI A,12
	      JRST .+1]
	LSH A,1
