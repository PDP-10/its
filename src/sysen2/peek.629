NAMA:	SETZ B,
	ROTC A,7
	JUMPE B,CPOPJ
	.IOT NTYO,B
	JRST NAMA

GPK1:	CTYPE 40
	DNUM 4,@USIPRQ	;OUTPUT # PG SWPIN RQ
	CTYPE 40
	MOVE A,@USWPRI
	IDIVI A,15000.	;CONVERT TO PG MILLISEC'S
	DNUM 5,A
	SKIPL @SUPPRO
	JRST TIMOT2	;NOT TOP LVL
	PUSHJ P,CRR
	ALIGN 10.
	ATYPE [ASCIZ /Total for tree /]
	MOVE B,@UTMPTR
	MOVE A,SYSBEG(B)
