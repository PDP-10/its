	      SETO C,
	      JRST .+1]
	TLNE A,%TCICO
	CTYPE "I
	TLNE A,%TCOCO
	CTYPE "O
	TLNE A,%TCRFS
	CTYPE "R
	TLNE A,%TCQRY
	CTYPE "Q	
	TLNE A,%TCRFT
	CTYPE "E
