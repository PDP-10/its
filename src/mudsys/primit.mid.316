	CAML	AB,[-5,,]	; SKIP IF GOOD ARRGS
	JRST	TFA
	SOJL	C,OUTRNG
	PUSH	TP,4(AB)
	PUSH	TP,5(AB)
	PUSHJ	P,(E)
	MOVE	A,(AB)		; RET STRUCTURE
	MOVE	B,1(AB)
	JRST	FINIS

