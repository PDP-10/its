	MOVEI	D,1(TP)		;POINT TO 2D ARG
	PUSH	TP,(P)
	MOVE	A,-3(TP)	;AND PUSH ITS POINTER
	PUSH	TP,(A)
	PUSHJ	P,IEQUAL
