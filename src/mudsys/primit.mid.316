; ARG CHECKER FOR PUT/GET/GETL

ARGS5:	HLRE	0,AB		; -# OF ARGS
	ASH	0,-1
	ADDI	0,2		; 0 OR -1 WIN
	JUMPG	0,TFA
	AOJL	0,TMA		; MORE THAN 3
	JRST	ARGS3		; GET ARGS

; PUT

MFUNCTION PUT,SUBR

	ENTRY
	MOVE	E,IIPUTP
	PUSHJ	P,ARGS5		; GET ARGS
	SKIPN	E,IPUTBL(E)
