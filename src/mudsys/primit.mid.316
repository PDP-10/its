	JRST	EQTMP1		; LOSER
	JRST	EQTMP2		; WINNER

EQTMP3:	AOS	-3(P)		; WIN RETURN
EQTMP1:	SUB	P,[3,,3]	; FLUSH JUNK
EQTMP4:	SUB	TP,[10,,10]
