INITGC:	SETOM	GCFLG
	SETZM	RCLV

;SAVE AC'S
	EXCH	PVP,PVSTOR+1
	IRP	AC,,[0,A,B,C,D,E,P,R,M,SP,TP,TB,AB,TVP,FRM]
	MOVEM	AC,AC!STO"+1(PVP)
