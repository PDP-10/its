	PUSHJ	P,OUTTP		; SEND IT OUT
	HRROI	C,PSAV-FSAV(C)	;POINT TO SAVED P
	MOVEI	B,TPDL
	PUSHJ	P,MARK1		;AND MARK IT
	PUSHJ	P,OUTTP		; SEND IT OUT
