;;;  LEXPR-FUNCALL, for old lisps which don't have it.   -*-lisp-*-


(defun LEXPR-FUNCALL n 
  (and (< n 1) (error '|0 args? - LEXPR-FUNCALL|))
  (let ((fun (arg 1)))
    (if (<= n 1)
	(funcall fun)
	(do ((i 2 (1+ i))
	     (l () (cons (arg i) l)))
	    ((= i n) 
	      (apply fun (nreconc l (arg n))))))))
	  