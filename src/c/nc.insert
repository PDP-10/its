TERMIN

DEFINE .PDATA
.SEG 3
TERMIN
	
; STACK HACKING FOR VARIABLE REFERENCES

%P==0
DEFINE PPUSH [A]
	PUSH P,A
	%P==%P+1
	TERMIN
DEFINE PPOP [A]
	POP P,A
	%P==%P-1
	TERMIN
DEFINE CCALL N,F
	.CCALL N,F
	%P==%P-N
	TERMIN

];END IF1

IF2,[IFDEF FS1,[
	.KILL %A,%P,A,B,C,D,P,GO,.CCALL
	]]
