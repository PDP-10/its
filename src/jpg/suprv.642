#-Multics
(DEFMACRO SYS-DEFAULTF (X) `(DEFAULTF ,X))

#+Multics
(DEFUN SYS-DEFAULTF (X)
       (SETQ X (NAMELIST X))
       (IF (EQ (CAR X) '*)
	   (DEFAULTF (CONS (CAR (DEFAULTF NIL)) (CDR X)))
	   (DEFAULTF X)))

