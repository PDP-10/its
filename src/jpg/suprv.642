	(MACSYMA-BREAK (*THROW 'MACSYMA-BREAK NIL))
	(MACSYMA-TOPLEVEL (COND (MBREAK (*THROW 'MBREAK NIL)) (T (MQUIT NIL))))
	(T (ERROR 'QUIT))))

