;
; RANDOM NUMBER GENERATOR (STOLEN FROM MUDDLE)
;
RELOCATABLE
TITLE RANDOM
.INSRT C;NC INSERT
.INSRT C;NM INSERT

CENTRY	SRAND,[SEED]
	MOVE	A,SEED
	ROT	A,-1
	MOVEM	A,RLOW
	RETURN

CENTRY	RAND

	MOVE	A,RHI
	MOVE	B,RLOW
	MOVEM	A,RLOW		;Update Low seed
	LSHC	A,-1		;Shift both right one bit
	XORB	B,RHI		;Generate output and update High seed
	MOVE	A,B
	RETURN

