	MOVE A,BPFPFW
	PUSHJ P,SYSFWP
	JRST SYSTCR
] ;IFE KA10P

PARTY2:	PUSHJ P,SYSTCR
IFN KS10P,[
	MOVEI I,[ASCIZ /ERR ADDR = /]
	PUSHJ P,SYSSP
	MOVE T,PARERA		; Error address is all we collect on the KS.
	PUSHJ P,SYSP
	PUSHJ P,SYSCRF
] ;KS10P
IFN KL10P,[
	SKIPN PARRSN
	 JRST PARTP1
	MOVEI I,[ASCIZ /ERR ADDR = /]
