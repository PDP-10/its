	JRST MEMTYP
] ;IFE KA10P

PARTYP:	MOVEI I,[ASCIZ /PARITY ERROR #/]	;PARITY ERROR
	MOVEI TT,0
MEMTYP:	PUSHJ P,SYSSP
	MOVE A,PARERR(TT)
	PUSHJ P,SYSOPT
	MOVEI I,[ASCIZ /,  PC = /]
	PUSHJ P,SYSSP
	MOVE A,PARPC(TT)
	PUSHJ P,SYSFWP
	MOVEI I,[ASCIZ /,  JOB # /]
	PUSHJ P,SYSSP
	MOVE A,PARUSR(TT)
	IDIVI A,LUBLK
	PUSHJ P,SYSOPT
	MOVEI I,[ASCIZ /, USR:/]
	PUSHJ P,SYSSP
	MOVE D,PARUSR(TT)
	MOVE A,UNAME(D)
	PUSHJ P,SYSSIX
	PUSHJ P,STYOS
	MOVE A,JNAME(D)
	PUSHJ P,SYSSIX
	JRST @.+1(TT)	;DISPATCH ON TYPE OF ERROR
	 PARTY2		;PARITY
	 SYSTCR		;NXM
IFE KA10P,[
	 BPFTY2		;BPF

BPFTY2:	MOVEI I,[ASCIZ /, PFW = /]
