(DEFUN FILECHECK (X) 
       ;;CHECK IF FILE X EXISTS ON DSK.  IF SO ASKS QUESTION. 
       (COND
	[ITS ((MEMQ (CADR X) '(< >)))]
	((NOT (APPLY 'UPROBE X)))
	((AND
	  (TYPE
	   (LIST
	    '";YOU HAVE"
	    X
	    '"ALREADY. WOULD YOU LIKE TO WRITE OVER IT? "))
	  (ASK)))
	((TYPE '";OK, YOUR OLD FILE IS SAFE." EOL)
	 NIL))) 

