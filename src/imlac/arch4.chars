SUBTTL CONTROL CHARACTERS

; -------- 0 - 37 (control characters) -------------

IFNDEF CTRLS,	CTRLS=0

; control characters that are normally visible

D040:
DSP:	INC E,D30 		; space (spaces may also be dark long vectors)
	INC D30,D30
	INC D20,X

DBS:	INC E,DM20		; backspace
	INC DM30,DM30
	INC DM30,X

DTAB:	DRJM			; computed beam-off long vector with
				; second word = TABVEC = 20000

DLFX:	DNOP			; for line feeds who don't want to be known as such
DLF:	INC E,D0M3		; line feed
	INC D0M3,D0M3
	INC D0M3,D0M3
	INC D0M3,D0M2
	INC X,X

DVT:	INC E,D03		; vertical tab
	INC D03,D03
	INC D03,D03
	INC D03,D02
	INC X,X

DCRX:	DNOP			; secretive CR
DCR:	DLXA LEFT		; CR with settable offset
DHOROF:	DLV D,-1,0
	DRJM

D033:	DNOP
	DJMS D117		; escape (altmode)
	DJMS DBS
	DJMS D044
	DRJM

