	INC B0M3,B0M1
	INC BM30,BM30
	INC BM30,BM20
	INC B03,B01
	INC D10,X


XFORM:	DJMS XXFORM		; teco cursor
	DJMS XXFORM
	DRJM

XXFORM:	INC E,DM3M3		; i-beam cursor
	INC DM3M3,DM20
	INC B30,B30
	INC B30,B30
	INC DM30,DM30
	INC B03,B03
	INC B03,B03
	INC B03,B03
	INC B03,B03
	INC DM30,DM30
	INC B30,B30
	INC B30,B30
	INC DM3M3,DM1M3
	INC D0M3,D0M3
	INC D0M3,D0M3
	INC X,X

DXHAIR:
	INC E,B30		; cursor used in tektronics cross hair simulation
	INC B30,B30
