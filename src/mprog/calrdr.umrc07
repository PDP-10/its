<DEFINE COMMAPRINT (STRUC "OPTIONAL" (TERP!-INITIAL T)) 
	#DECL ((STRUC) STRUCTURED)
	<AND .TERP!-INITIAL <TERPRI>>
	<MAPF <>
	      <FUNCTION (X) 
		      <COND (<TYPE? .X SYMBOL> <PRINC <1 .X>>) (<PRIN1 .X>)>
		      <PRINC ", ">>
	      .STRUC>>

