; "rsubr to find the length of a file"

<TITLE *FILE-LENGTH>
	<SYMDEF *CALL #OPCODE *43000000000*>
	<DECLARE ("VALUE" <OR FIX FALSE> CHANNEL)>
	<MOVE	A* (AB) 1>
	<MOVE	B* (A) 21>	; "status slot"
	<ANDI	B* *77*>
	<CAIE	B* *43*>
	<JRST	NOTDSK>
	<*CALL	FILLEN>
	<JRST	FAIL>
	<MOVSI	A* TFIX>
	<JRST	FINIS>

NOTDSK	<MOVE	A* <TYPE-WORD FIX>>
	<SETZ	B*>
	<JRST	FINIS>

FAIL	<MOVSI	A* TFALSE>
	<SETZ	B*>
	<JRST	FINIS>

FILLEN	<SETZ>
	*465154544556*		; "SIXBIT /FILLEN/"
	<(A) 1>			; "io channel #"
	<SETZM B>
F