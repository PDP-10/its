	PDPT A		;OUTPUT REEL NUMBER
	JRST CRR

;COMMAND TO LIST THE MFD
AMFD:	PUSHJ P,DEVGET
	SETOM MFDIN
AMFD1:	PUSHJ P,MFDN
	 POPJ P,
	PSIS USNM
	PUSHJ P,CRR
	JRST AMFD1

;COMMAND TO LIST A DIRECTORY
ALISTF:	SETOM FLNI
	CLEARM MICFLG
	MOVEI A,(SIXBIT /DSK/)
	HRRM A,FNO
GETDIR:	PRINT DIRECTORY=
