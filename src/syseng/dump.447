GFPDSK:	MOVE SYM,['SECOND]
	PUSHJ P,GFPDV1
GFPTO:	 PUSHJ P,GFPDEV
	MOVEM SYM,GFRDEV
	POPJ P,

GFPSEC:	MOVE SYM,['SECOND]
	PUSHJ P,GFPDV1
GFPFRM:	 PUSHJ P,GFPDEV
	IORM B,GFRPNM
	POPJ P,

;RETURN WITH SIXBIT DEVICE IN SYM AND BIT MASK OF PACK NUMBERS IN B
GFPDEV:	PRINT [DEVICE=]
	PUSHJ P,GETSYL
	 JRST RET
	CAIA
GFPDV1:	 AOS (P)
	MOVEI B,0
	MOVSI A,-NQS
GFPDV2:	CAME SYM,QRESRV(A)
