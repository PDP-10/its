;LISTING ROUTINES

;LIST LINK INFO
LDMUT5:	PSIS [SIXBIT /L/]
	PSIS PACKN	; SYSNM
	PSIS LNKNM1
	PSIS LNKNM2
	POPJ P,

;WHEN LOADING, LIST SNAMES AS ENCOUNTERED
