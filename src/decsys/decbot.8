IFNDEF DECS1,[
TITLE DECBOOT	;BOOT IN DECUUO

LOC 45		;JUST ABOVE .JBREL, IN A GAP IN THE JOBDAT AREA
]
.ELSE [
BOTBOT::OFFSET 45-.
]

BOTCHAN==1

BOTBEG:: 0+IFDEF DECS1,JRST DECS1
BOTGO::	.OPEN BOTCHAN,BOTFILE
	 .LOSE 1000
	.CALL BOTLOAD
	 .LOSE 1000
	.IOT BOTCHAN,1
	.CLOSE BOTCHAN,
	MOVE 17,BOTSWITCH
	JRST 1(1)	;GO OFF TO DECUUO'S SPECIAL START ADDR TO GOBBLE OUR SWITCH WD.

BOTSWI::40		;(DECBT)
BOTFIL::4,,SIXBIT/   SYSTS    DEC/
BOTLOA::SETZ
	SIXBIT /LOAD/
	1000,,-1
	SETZI BOTCHAN

IFNDEF DECS1,END BOTGO

BOTEND::OFFSET 0
