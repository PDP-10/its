(DEFUN FINDNUM (X) 
       (COND ((NUMBERP X) X)
	     ((EQ (CAR X) 'EXACTLY)
	      (LIST (CADR X) (ADD1 (CADR X)) NIL))
	     ((EQ (CAR X) '>) (ADD1 (CADR X)))
	     ((EQ (CAR X) '<) (CADR X))
	     ((EQ X 'NS) 1.)
	     ((EQ X 'NPL) 2.)
	     ((ERT FINDNUM))))



;;;############################################################

(DEFUN FINDREDUCE (X Y) 
       (PROG NIL 
	UP   (SETQ X (CDR X))
	     (COND ((ZEROP (SETQ Y (SUB1 Y))) (RETURN X)) ((GO UP)))))



