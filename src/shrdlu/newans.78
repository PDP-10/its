(DEFUN NAMESIZE (X) 
       (OR (NUMBERP X) (SETQ X (APPLY 'PLUS X)))		       ;ACCEPTS EITHER SINGLE NUMBER OR LIST OF
       (COND ((GREATERP X 383.) 'LARGE)				       ;DIMENSIONS.
	     (T 'SMALL)))



