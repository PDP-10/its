(DEFUN MUNG (LIST MUNG) 
       (SETQ MUNG (LIST 'QUOTE MUNG))
       (AND DISCOURSE (SETQ LIST (CADDR LIST)))
       (COND ((EQ (CAAR (CDDDR LIST)) 'THAMONG)
	      (RPLACD (CDAR (CDDDDR LIST)) MUNG))
	     ((RPLACD (CDDDR LIST)
		      (CONS (LIST 'THAMONG
				  (LIST 'THV
					(CADR (CADDR LIST)))
				  MUNG)
			    (CDDDDR LIST))))))



