(DEFUN PLURALIZE (ITEM NUM) 

       ;;CONVERTS A SINGULAR NOUNPHRASE OR "ONCE" STATEMENT INTO
       ;;PLURAL.
       (COND ((GREATERP 2. NUM) ITEM)
	     (T (COND ((MEMQ 'A (CAR ITEM))
		       (CONS (PLURALMAKE (SUBST (NAMENUM NUM)
						'A
						(CAR ITEM)))
			     (CDR ITEM)))
		      ((MEMQ 'ONCE (CAR ITEM))
		       (CONS (SUBST (ORDNAME NUM)
				    'ONCE
				    (CAR ITEM))
			     (CDR ITEM)))
