(DEFUN TOPLEVEL (EVENT) 

       ;;FINDS THE TOP LEVEL EVENT GOING ON AT THE TIME
       (COND ((EQ (GET EVENT 'WHY) 'COMMAND) EVENT)
	     (T (TOPLEVEL (GET EVENT 'WHY)))))



