(DEFUN THV1
 (X)								       ;(THV1 'X) IS THE VALUE OF THE PLANNER VARIABLE
 (SETQ THXX X)							       ;$?X RETURNS ERROR MESSAGE IF X UNBOUND OR
 (COND ((EQ (SETQ X (CADR (SASSQ X				       ;UNASSIGNED
				 THALIST
				 (FUNCTION (LAMBDA NIL
						   (PRINT THXX)
						   (THERT THUNBOUND
							  -
							  THV1))))))
	    (QUOTE THUNASSIGNED))
	(PRINT THXX)
	(THERT THUNASSIGNED - THV1))
       (T X)))

