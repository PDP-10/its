		    (SETQ THTREE1 (CDR THTREE1)))
		   ((EQ (CAR THX) (QUOTE THREMBIND))
		    (SETQ THTREE1 (CDR THTREE1)))
		   ((RPLACD THTREE1 (CDDR THTREE1))))
	     (COND ((EQ (CAR THX) (CAR THA)) (GO DONE)))
	     (GO PLUP)
	RTLEV(SETQ THX (CDDR THX))
	LEVLP(COND ((NULL (CAR THX)) (SETQ THTREE1 (CDR THTREE1))
