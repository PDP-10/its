(DEFUN THMESSAGEF NIL (PROG (BOD)
			    (SETQ BOD (CAR THTREE))
			    (THPOPT)
			    (COND ((AND (THBIND (CADR BOD))
					(THMATCH1 (CADDR BOD)
						  THMESSAGE))
				   (THPUSH THTREE (LIST (QUOTE THPROG)
							(CDDR BOD)
							NIL
							(CDDR BOD)))
				   (SETQ THMESSAGE NIL)
				   (RETURN (THPROGA)))
				  (T (SETQ THALIST THOLIST) ))
			    (RETURN NIL)))

