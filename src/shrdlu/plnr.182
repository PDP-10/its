       (ATO VAL IND)
       (THPUSH THTREE
	       (LIST (QUOTE THMUNG)
		     (LIST (LIST (QUOTE PUTPROP)
				 (LIST (QUOTE QUOTE) ATO)
				 (LIST (QUOTE QUOTE) (GET ATO IND))
				 (LIST (QUOTE QUOTE) IND)))))
       (PUTPROP ATO VAL IND))

