CRSPRT:	;CROSS PRODUCT OF TWO 3-TUPLES
	PUSH P,E
	PUSH P,F
	PUSH P,G

	MOVE E,A
	MOVE F,B
	MOVE G,C

.F ZR(E)=A(F)*B(G)-B(F)*A(G)
.F A(E)=B(F)*ZR(G)-ZR(F)*B(G)
.F B(E)=ZR(F)*A(G)-A(F)*ZR(G)

