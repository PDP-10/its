		;MATRIX AND VECTOR ROUTINES

MATVCT:	;MULTIPLY MATRIX BY VECTOR
;ANSWER,MATRIX,VECTOR,N,M  N-ROWS
	CAMN A,C
	STOP _INLINE-MATVCT
	PUSH P,A
	MOVNS E
	HRL C,E
	PUSH P,C
