	SETZM (A)
RWLOSP:	MOVE E,(B)
	FMPR E,(C)
	FADRM E,(A)
	AOS B
	AOBJN C,RWLOSP
	AOS A
	SOJG D,CLLOSP
	SUB P,[1,,1]
	JRST POPAJ

VCTSCL:	;MULTIPLY VECTOR OR MATRIX BY SCALAR
;ANSWER,VECTOR,SCALAR,N
	PUSH P,A
	MOVNS D
	HRL B,D
