REG T,2		; Name pointer
TT==T+1		; Terminator character

	; Write DEV: DIR; FN1 FN2 to NAMBUF
	; Never skips
NAMPRN:	MOVE T,[ASCBP,,NAMBUF]		; Scratch here
	MOVEI TT,":
	SKIPE FILDEV			; If there's a DEV
	 PUTFN T,FILDEV			;  then write DEV:
	MOVEI TT,";
