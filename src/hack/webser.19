REG C		; Character to check
REG REWIND	; 0 to seek to

	; Skip if the open file looks like it contains HTML
IS.HTM:	.IOT FILI,C			; Look at the first char
	SETZ REWIND,
	.ACCESS FILI,REWIND		; Rewind the file
