	.STITL MACROS
.MACRO	FPUSH X
	.XLIST
	MOV X,(F)+
	.LIST
.ENDM

.MACRO	PUSH X
	.XLIST
	MOV X,-(P)
	.LIST
.ENDM

.MACRO	POP X
	.XLIST
	MOV (P)+,X
	.LIST
.ENDM

.MACRO	TEXT X
	.XLIST
	.ASCII ØŠ	.BYTE 0
	.EVEN
	.LIST
.ENDM

.MACRO	TYPEIT X
	.XLIST
	JSR F,TXTYPE
	TEXT ^ØŠ	.LIST
.ENDM

.MACRO	ERROR X
	.XLIST
	JSR PC,ERRORX
	TYPEIT ^/Error--'X/
	JSR PC,@(P)+
	.LIST
.ENDM

.MACRO	ENTERR X
	.XLIST
	JSR PC,ERRORX
	TYPEIT ^/Error--entry /
	MOV ENTRYP,B
	JSR PC,NAMOUT
	TYPEIT ^/X/
	JSR PC,@(P)+
	.LIST
.ENDM

.MACRO	BUG X
	.XLIST
	JSR PC,ERRORX
	TYPEIT ^/Bug--'x/
	JSR PC,@(P)+
	.LIST
.ENDM

.MACRO	MSG X
