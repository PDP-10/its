BCKUP1:	MOV #DSKBUF,B	;WHERE TO PUT DATA
	JSR PC,DSKRED	;READ A CYLINDER
	CMP #-1,NDISK
	BEQ 1$		;FOR SCAN
	MOV BLOKNO,A	;WHERE TO PUT IT
