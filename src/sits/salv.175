	SOB F,TENRE1
	CLR TENFUN	;DONE
	JSR PC,TENWAIT
	RTS PC

TENWRT:	MOV #1,TENFUN	;SAY WE WANT TO WRITE
	JSR PC,TENWAIT	;GO SAY WE ARE READY AND WAIT FOR TEN
