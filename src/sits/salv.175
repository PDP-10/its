1$:	ADD BACKIN,BLOKNO	;GET THE NEXT CYLINDER
	SOB E,BCKUP1	;DO IT FOR ALL THE CYLINDERS
	MOV #-1000,TRNSLE	;RESTORE THE BLOCK LENGTH
	RTS PC

