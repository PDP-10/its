	.STITL !THE! !SALVAGER!!!
SALV:	RESET		;INIT THE BUSS
	MOV #PDL,P	;INIT THE STACK POINTER
	MOV #-1000,TRNSLE	;INIT TRANSFER LENGTH

.IFNZ SGMNTR
	JSR PC,SEGSET	;TURN ON SEGMENTER
	JSR PC,MEMTST	;FIND NO. 512-WORD BLKS IN HIGH MEMORY
.ENDC
	JSR PC,ITYI	;INIT THE TYI
	TYPEIT ^/_SALV /	;_ MAKES A CRLF
	MOV #VERN,B
	JSR PC,DECOUT
SALV2:	MOV #PDL,P
.IF NZ FLOP
	JSR PC,DSKOFF
	CLR SMSLGT
.ENDC
	TYPEIT ^/_*/
SALV5:	JSR PC,TYI1
	MOV A,B
	ASL B
.IIF NZ FLOP,	JSR PC,DISKON
	JSR PC,@DSPTAB(B)
	BR SALV2

SALVN:	SUB #'0,A
	MOV A,WCHDSK	;SET THE VARIABLE
	BR SALV5
SALVQ:	MOV #PDL,P
	TYPEIT ^/ ?/
	BR SALV2
.IFZ 105
FXB0I0:	TYPEIT ^/ixed head disk block 0 init_sure? /
	JSR PC,TYI1
	CMP #' ,A
	BEQ GOFXB0
	CMP #'Y,A
	BNE SALVQ
GOFXB0:	JSR PC,CRLF
	JMP FXINIT
.ENDC
INIT0:	TYPEIT ^/nit_sure?/
	JSR PC,TYI1
	CMP #' ,A
	BEQ GOINIT
	CMP #'Y,A
	BNE SALVQ
GOINIT:	JSR PC,CRLF
	JMP INITD

CHECK0:	TYPEIT ^/heck_/
	JSR PC,CHECK
	JMP SALV2

GOGOGO:	MOV #4,-(P)
	CLR WCHDSK	
1$:	JSR PC,CHECK
	INC WCHDSK
	DEC (P)
	BNE 1$
.IFZ LSI
	JMP 157002	;EXIT TO SITS
.IFF
	JMP 173000	;FIX LATER
.ENDC

.IFNZ FLOP
