	ASH #-3,A	;BLOCK # -> BYTE OFFSET
	BIC #1,A	;-> WORD OFFSET IN BIT TABLE
	MOV A,(D)+	;MAKES OFFSET WORD OF DESCR FILE
	RTS PC

DESCRX:	TYPEIT ^/_Descr file overflowed_/
