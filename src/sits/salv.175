TYLRB2:	MOVB @TBUFPT,A	;TYPE RUBBED OUT CHAR
	JSR PC,TYPE
	TST NCHR	;FIRST CHAR RUBBED OUT?
	BNE TYLIN2	;NO, DONE WITH RUBOUT
	MOV #'\,A	;YES, CLOSE RUBBING MODE
