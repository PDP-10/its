BLOCKT:	TYPEIT <lock at a time copies>
	MOV #-1000,BACKTL
.IIF NZ FLOP,	MOV #247.,BACKCN
.IIF  Z FLOP,	MOV #203.*12.,BACKCN
	MOV #1,BACKIN
	JMP SALV2

TRACKT:	TYPEIT <rack at a time copies>
	JSR PC,TRKSET
	JMP SALV2

TRKSET:
.IIF NZ FLOP,	MOV #-13.*1000,BACKTL
.IIF  Z FLOP,	MOV #-14000,BACKTL
.IF Z FLOP
	MOV #203.,BACKCN
	MOV #12.,BACKIN
.IFF
	MOV #19.,BACKCN
	MOV #13.,BACKIN	;SPECIAL FLOPPY MODE
.ENDC	
	RTS PC

.IFNZ FLOP
SCAN:	TYPEIT <can
>
	JSR PC,TRKSET
	MOV #-1,NDISK
	JSR PC,BACKUP
	JMP SALV2
